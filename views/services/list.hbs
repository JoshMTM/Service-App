<h1>Provided services</h1>
<table class="table service-list">
	<thead>
		<tr>
			<th>Name</th>
			<th>Service Provider</th>
			<th>Description</th>
			<th>Address</th>
			<th>Time</th>
			<th>Price</th>
			<th></th>
		</tr>
	</thead>
	<tbody>
		{{#each services as |service|}}
			<tr>
				<td>{{service.name}}</td>
				<td>{{firstName}} {{lastName}}</td>
				<td>{{service.description}}</td>
				<td>{{service.address}}</td>
				<td>{{service.time}}</td>
				<td>{{service.price}}</td>
				<td>
					<a href="/services/{{service.id}}">Detail</a>
					<a href="/api/services/delete/{{service.id}}">Delete</a>
				</td>
			</tr>
		{{/each}}
	</tbody>
</table>

